<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<VBox spacing="10" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="frontend.LabeledFuzzySetListController">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10"/>
    </padding>
    <Button text="Unselect" onAction="#unselect"/>
    <HBox spacing="10">
        <ListView fx:id="labeledFuzzySetList" VBox.vgrow="ALWAYS"/>
        <fx:include source="labeled-fuzzy-set/labeled-fuzzy-set-editor.fxml" fx:id="labeledFuzzySetEditor"/>
    </HBox>
</VBox>
